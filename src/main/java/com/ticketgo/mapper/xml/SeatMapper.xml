<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ticketgo.mapper.SeatMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.ticketgo.entity.Seat">
        <id column="seat_id" property="seatId" />
        <result column="showing_id" property="showingId" />
        <result column="row_index" property="rowIndex" />
        <result column="col_index" property="colIndex" />
        <result column="status" property="status" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        seat_id, showing_id, row_index, col_index, status
    </sql>

</mapper>
